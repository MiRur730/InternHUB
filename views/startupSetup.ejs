<%- include("./layouts/header.ejs")  %>

<div class="container">
    <form action="/startup-setup" method="POST" enctype='application/x-www-form-urlencoded'>
        <div class="row">
            <div class="col-sm-20">
                <input type="text" name="cofounder" placeholder="Enter Company cofounder name " required>
            </div>
        </div>

        <div class="row mt-3">
            <div class="col-sm-20">
                <input type="text" placeholder="Enter Startup Company Name" name="Cname" required>
            </div>
        </div>

        <div class="row mt-3">

            <div class="col-sm-20">
                <textarea name="hiringcontent" id="" cols="30" rows="10" placeholder="Enter Hiring Post here here" required></textarea>
            </div>
        </div>

       

        

        <div class="row mt-3">

            <div class="col-sm-20">
                <input type="text" name="name" placeholder="Enter admin name" required>
            </div>
        </div>

        <div class="row mt-3">

            <div class="col-sm-20">
                <input type="email" name="email" placeholder="Enter email" required>
            </div>
        </div>
        <div class="row mt-3">

            <div class="col-sm-20">
                <input type="password" name="password" placeholder="Enter password name" required>
            </div>
        </div>
        


        <div class="row mt-3">

            <div class="col-sm-20">
                <input type="submit" class="btn btn-primary" value="Startup Setup">
            </div>
        </div>
    </form>
</div>

<% 
if(typeof message !== "undefined"){
    %>
<p style="color:red"> <%= message %></p>
    <%
}

%>
<%- include("./layouts/footer.ejs")  %>